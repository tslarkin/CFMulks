    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/CFMulks/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
        },
        {
            "name": "Python Debugger: Custom Command",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/CFMulks/manage.py",
            "args": ["update_notebooks"]
        }
    ]
}